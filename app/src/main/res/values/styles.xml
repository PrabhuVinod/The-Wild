<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:fontFamily">@font/interui</item>
        <item name="fontFamily">@font/interui</item> <!-- target android sdk versions < 26 and > 14 if theme other than AppCompat -->
    </style>

    <style name="SplashStyle" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/green_blur</item>
        <item name="android:fontFamily">@font/buffaloscript</item>
        <item name="fontFamily">@font/buffaloscript</item> <!-- target android sdk versions < 26 and > 14 if theme other than AppCompat -->
    </style>

</resources>
